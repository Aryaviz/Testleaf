package org.college;

public class College {
	public void collegeName() {
		System.out.println("College Name is Christ University");
	    }
		public void collegeCode() {
			System.out.println("College code is SCSVMV");
		}
		public void collegeRank() {
			System.out.println("College Rank is A+");{
				}
			}

}
***************************************************
package org.department;

import org.college.College;

public class Department extends College {
	public void deptName() {
		System.out.println("Dept Name is Christ EEE");
	    }
}

**************************************************
package org.student;

import org.department.Department;

public class Student extends Department {

	public void studentName() {
		System.out.println("studentName is Arya");
	    }
	public void studentDept() {
		System.out.println("Dept Name is  EEE");
	    }
	public void studentId() {
		System.out.println("StudentId is 310308");
	    }
	
	
}
********************************************
package week3.day2;

import org.student.Student;

public class Multilevel_inheritance extends Student

{

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Student  stud =new Student();
		stud.studentDept();
		stud.studentId();
		stud.studentName();
		stud.collegeName();
		stud.collegeCode();
		stud.collegeRank();
		stud.deptName();
	}

}
**************************************
output:

Dept Name is  EEE
StudentId is 310308
studentName is Arya
College Name is Christ University
College code is SCSVMV
College Rank is A+
Dept Name is Christ EEE















